{
  "version": 2,
  "buildCommand": "echo 'No build step required'",
  "outputDirectory": ".",
  "public": true,
  "routes": [
    { "src": "/api/tests", "dest": "/api/tests.js" },
    { "src": "/api/images", "dest": "/api/images.js" },
    { "src": "/api/airtable-check", "dest": "/api/airtable-check.js" },
    { "src": "/api/tests-image-debug", "dest": "/api/tests-image-debug.js" }
  ],
  "functions": {
    "api/tests.js": {
      "memory": 1024,
      "maxDuration": 20
    },
    "api/images.js": {
      "memory": 1024,
      "maxDuration": 10
    },
    "api/airtable-check.js": {
      "memory": 512,
      "maxDuration": 5
    },
    "api/tests-image-debug.js": {
      "memory": 1024,
      "maxDuration": 30
    }
  },
  "github": {
    "silent": true
  }
}