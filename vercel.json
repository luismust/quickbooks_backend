{
  "version": 2,
  "buildCommand": "echo 'No build step required'",
  "outputDirectory": ".",
  "public": true,
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/api/(.*)",
      "headers": {
        "Access-Control-Allow-Credentials": "true"
      },
      "continue": true
    },
    { "src": "/api/tests", "dest": "/api/tests.js", "methods": ["GET", "POST", "OPTIONS"] },
    { "src": "/api/images", "dest": "/api/images.js", "methods": ["GET", "OPTIONS"] },
    { "src": "/api/airtable-check", "dest": "/api/airtable-check.js", "methods": ["GET", "OPTIONS"] },
    { "src": "/", "dest": "/api/index.js" },
    { "src": "/(.*)", "status": 404, "dest": "/api/index.js" }
  ],
  "functions": {
    "api/tests.js": {
      "memory": 1024,
      "maxDuration": 20
    },
    "api/images.js": {
      "memory": 1024,
      "maxDuration": 10
    },
    "api/airtable-check.js": {
      "memory": 512,
      "maxDuration": 5
    },
    "api/index.js": {
      "memory": 512,
      "maxDuration": 10
    }
  },
  "github": {
    "silent": true
  }
}