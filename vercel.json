{
  "version": 2,
  "routes": [
    {
      "src": "/(.*)",
      "methods": ["OPTIONS"],
      "dest": "/api/cors-options.js",
      "headers": {
        "Access-Control-Allow-Credentials": "true",
        "Access-Control-Allow-Origin": "https://quickbooks-test-black.vercel.app",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Authorization, X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Date, X-Api-Version"
      }
    },
    {
      "src": "/api/(.*)",
      "headers": {
        "Access-Control-Allow-Credentials": "true",
        "Access-Control-Allow-Origin": "https://quickbooks-test-black.vercel.app",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Authorization, X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Date, X-Api-Version"
      }
    },
    { "src": "/api/debug", "dest": "/api/debug.js" },
    { "src": "/api/debug-post", "dest": "/api/debug-post.js" },
    { "src": "/api/tests-simple", "dest": "/api/tests-simple.js" },
    { "src": "/api/airtable-check", "dest": "/api/airtable-check.js" },
    { "src": "/api/tests", "dest": "/api/tests.js" },
    { "src": "/api/tests/(.*)", "dest": "/api/tests/[id].js?id=$1" }
  ],
  "functions": {
    "api/tests.js": {
      "memory": 1024,
      "maxDuration": 10
    },
    "api/tests-simple.js": {
      "memory": 1024,
      "maxDuration": 10
    },
    "api/cors-options.js": {
      "memory": 128,
      "maxDuration": 2
    },
    "api/debug.js": {
      "memory": 128,
      "maxDuration": 2
    },
    "api/debug-post.js": {
      "memory": 128,
      "maxDuration": 5
    }
  }
}